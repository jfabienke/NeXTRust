{
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [{
          "type": "command",
          "command": "./hooks/dispatcher-v2.sh pre",
          "timeout": 90
        }]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Bash",
        "hooks": [{
          "type": "command",
          "command": "./hooks/dispatcher-v2.sh post",
          "timeout": 300
        }]
      }
    ],
    "Stop": []
  }
}